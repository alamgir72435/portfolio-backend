	<div id="about" class="section">
		<div class="container">
			<div class="row">
				<div class="col-md-5">
					<img class="aboutImage" src="{{utility.aboutPhoto}}">
				</div>
				<div class="col-md-7 personal">
					<div class="personal-info-inner">
						{{#if utility}}
						<h4>{{ utility.fullName }}</h4>
						<h3>{{ utility.designation }}</h3>
						<p>
							{{ utility.description }}
						</p>
						{{/if}}
						<ul class="socials">
							<li>
								<a target="_blank" href="{{ utility.facebook }}">
									<i class="fab fa-facebook"></i>
								</a>
							</li>
							<li>
								<a target="_blank" href="{{ utility.twitter }}">
									<i class="fab fa-twitter"></i>
								</a>
							</li>
							<li>
								<a target="_blank" href="{{ utility.instagram }}">
									<i class="fab fa-instagram"></i>
								</a>
							</li>
							<li>
								<a target="_blank" href="{{ utility.youtube }}">
									<i class="fab fa-youtube"></i>
								</a>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
