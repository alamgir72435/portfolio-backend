	<div id="stats" class="statsSection section">
		<div class="container">
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="squareItem">
						<div class="squareInnerContainer">
							<div class="squareIcon">
								<i class="fa fa-users">
									
								</i>
							</div>
							<div class="squareContent">
								<h2 class="counter">{{ state.client }}</h2>
								<h3>Clients</h3>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-3 col-sm-6">
					<div class="squareItem">
						<div class="squareInnerContainer">
							<div class="squareIcon">
								<i class="fa fa-clock">
									
								</i>
							</div>
							<div class="squareContent">
								<h2>
									{{ state.project }}
								</h2>
								<h3>Projects</h3>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-3 col-sm-6">
					<div class="squareItem">
						<div class="squareInnerContainer">
							<div class="squareIcon">
								<i class="fa fa-tablet">
									
								</i>
							</div>
							<div class="squareContent">
								<h2>{{ state.app }}</h2>
								<h3>Apps</h3>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-3 col-sm-6">
					<div class="squareItem">
						<div class="squareInnerContainer">
							<div class="squareIcon">
								<i class="fa fa-calendar">
									
								</i>
							</div>
							<div class="squareContent">
								<h2>{{ state.website }}</h2>
								<h3>Websites</h3>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div id="contact" class="contactSection section">
		<div class="container">
			<div class="row">
				<div class="col-md-6 mx-auto text-center">
					<div class="subHeading">
						<span class="msg"></span>
						<h3 class="text-left">Feel free to send Message</h3>
						<form action="">
							<div class="row">
								<div class="col-md-6">
									<div class=" text-left form-group">
										<label for="Name">Name</label>
										<input id="name" placeholder="Name" class="form-control">
									</div>
								</div>
								<div class="col-md-6">
									<div class=" text-left form-group">
										<label for="email">Email</label>
										<input  id="email" placeholder="example@gmail.com" class="form-control">
									</div>
								</div>
								<div class="col-md-12">
									<div class="text-left form-group">
										<label for="subject">Subject</label>
										<input id="subject" placeholder="subject" class="form-control">
									</div>
								</div>
								<div class="col-md-12">
									<div class="text-left form-group">
										<label for="message">message</label>
										<textarea id="message" placeholder="message" class="form-control" cols="30" rows="3"></textarea>
									</div>
								</div>
								<div class="mt-3 col-md-12">
									<div class="text-left form-group">
										<button id="sendMessage" class="btn btn-secondary">Send Message</button>
									</div>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>

